<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd">
	<changeSet id="UNIONVMS-4822-movement-CLIENT_ID" author="nikospara">
		<insert tableName="settings">
			<column name="setting_id" valueSequenceNext="hibernate_sequence" />
			<column name="setting_key" value="eu.europa.ec.fisheries.uvms.plugins.flux.movement.CLIENT_ID"/>
			<column name="setting_value" value="flux-movement-plugin"/>
			<column name="global" value="0"/>
			<column name="setting_description" value="The connectorID to send to FLUX TL"/>
			<column name="setting_last_modified" valueDate="CURRENT_TIMESTAMP"/>
			<column name="setting_module_id" valueComputed="(SELECT module_id FROM modules WHERE module_name='exchange')"/>
			<column name="updated_by" value="UVMS"/>
		</insert>

		<rollback>
			<delete tableName="settings">
				<where>setting_key=:value</where>
				<whereParams>
					<param name="value" value="eu.europa.ec.fisheries.uvms.plugins.flux.movement.CLIENT_ID" />
				</whereParams>
			</delete>
		</rollback>
	</changeSet>
</databaseChangeLog>
